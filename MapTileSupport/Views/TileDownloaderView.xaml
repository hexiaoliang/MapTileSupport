<Page x:Class="MapTileSupport.Views.TileDownloaderView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MapTileSupport.Views"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="TileDownloaderView">

    <StackPanel Orientation="Vertical">
        <Button Style="{StaticResource NoMouseOverButtonStyle_body}" Margin="0, 30">选择级别</Button>
        <Button Style="{StaticResource NoMouseOverButtonStyle_body}">开始下载</Button>
        <Button Style="{StaticResource NoMouseOverButtonStyle_body}" Margin="0, 30">停止下载</Button>
    </StackPanel>
</Page>
